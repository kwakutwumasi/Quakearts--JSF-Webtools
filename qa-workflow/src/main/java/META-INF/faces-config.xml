<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xml="http://www.w3.org/XML/1998/namespace" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd ">
	<managed-bean>
		<managed-bean-name>formFields</managed-bean-name>
		<managed-bean-class>com.quakearts.workflowapp.jbpm.tag.listener.beans.SearchFormFieldsBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<component>
		<component-type>com.quakearts.facelets.output.upload</component-type>
		<component-class>com.quakearts.workflowapp.jbpm.tag.output.FileUploadComponent</component-class>
	</component>
	<render-kit>
		<renderer>
			<component-family>com.quakearts.facelets.output</component-family>
			<renderer-type>com.quakearts.facelets.output.fileupload.renderer</renderer-type>
			<renderer-class>com.quakearts.workflowapp.jbpm.tag.output.FileUploadRenderer</renderer-class>
		</renderer>
	</render-kit>
	<component>
		<component-type>com.quakearts.facelets.output.download</component-type>
		<component-class>com.quakearts.workflowapp.jbpm.tag.output.FileUploadComponent</component-class>
	</component>
	<render-kit>
		<renderer>
			<component-family>com.quakearts.facelets.output</component-family>
			<renderer-type>com.quakearts.facelets.output.filedownload.renderer</renderer-type>
			<renderer-class>com.quakearts.workflowapp.jbpm.tag.output.FileDownloadRenderer</renderer-class>
		</renderer>
	</render-kit>
</faces-config>
