		<attribute>
			<name>timestyle</name>
		</attribute>
		<attribute>
			<name>transient</name>
		</attribute>
	</tag>
	<tag>
		<tag-name>fileNameValidator</tag-name>
		<handler-class>com.quakearts.webapp.facelets.tag.input.validator.FileNameValidatorTagHandler</handler-class>
