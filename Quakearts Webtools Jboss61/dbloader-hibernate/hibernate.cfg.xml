        <property name="hibernate.bytecode.use_reflection_optimizer">false</property>
        <property name="hibernate.connection.driver_class">com.microsoft.sqlserver.jdbc.SQLServerDriver</property>
        <property name="hibernate.connection.password">Password1</property>
        <property name="hibernate.connection.url">jdbc:sqlserver://172.19.1.136:1433;databaseName=JarDB</property>
        <property name="hibernate.connection.username">sa</property>
        <property name="hibernate.dialect">org.hibernate.dialect.SQLServerDialect</property>
        <property name="hibernate.search.autoregister_listeners">false</property>
        <mapping resource="com/quakearts/tools/classloaders/hibernate/JarFile.hbm.xml" />
        <mapping resource="com/quakearts/tools/classloaders/hibernate/JarFileEntry.hbm.xml" />
    </session-factory>
</hibernate-configuration>
